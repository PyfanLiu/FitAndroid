<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.wjz.www.actionbarandsearchable">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".barActivity.ActionButtonActivity"></activity>
        <activity android:name=".barActivity.ActionModeActivity"></activity>
        <activity android:name=".barActivity.NavigationActivity"></activity>
        <activity android:name=".barActivity.TitleActivity"></activity>
    </application>

</manifest>