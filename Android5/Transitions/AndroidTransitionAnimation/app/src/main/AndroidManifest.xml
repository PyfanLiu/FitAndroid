<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wjz.androidtransitionanimation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".transition.TransitionActivity"></activity>
        <activity android:name=".transition.TransitionExplodeActivity"></activity>
        <activity android:name=".VectorActActivity"></activity>
        <activity android:name=".textv.TextViewActActivity"></activity>
        <activity android:name=".indicate.IndicateActActivity"></activity>
    </application>

</manifest>